{
  "name": "conzia",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "node --test tests/*.test.ts",
    "typecheck": "tsc --noEmit",
    "build": "npm run typecheck && vite build",
    "build:android": "npm run typecheck && vite build --mode android",
    "android:sync": "node scripts/sync-android-assets.mjs",
    "android:prepare": "npm run build:android && npm run android:sync",
    "android:apk": "npm run android:prepare && cd android && ./gradlew assembleDebug",
    "docs:function-map": "node scripts/generate-function-map.mjs",
    "docs:test-questions": "node scripts/generate-test-questions.mjs",
    "preview": "vite preview"
  },
  "dependencies": {
    "@playwright/test": "^1.58.2",
    "bullmq": "^5.68.0",
    "framer-motion": "^11.18.2",
    "ioredis": "^5.9.3",
    "lucide-react": "^0.469.0",
    "node-cron": "^4.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "recharts": "^3.7.0",
    "stripe": "^20.3.1",
    "vitest": "^4.0.18"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vite": "^5.4.11"
  }
}
